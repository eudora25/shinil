{
  "name": "shinil-project",
  "version": "1.0.0",
  "description": "Shinil Project API Server",
  "type": "module",
  "scripts": {
    "api": "cd vue-project && node server.js",
    "api:dev": "cd vue-project && npm run dev:api",
    "api:pm2": "pm2 start ecosystem.config.js",
    "api:pm2:stop": "pm2 stop shinil-api-server",
    "api:pm2:restart": "pm2 restart shinil-api-server",
    "api:pm2:logs": "pm2 logs shinil-api-server",
    "api:pm2:status": "pm2 status",
    "start": "cd vue-project && node server.js",
    "dev": "cd vue-project && npm run dev",
    "build": "cd vue-project && npm run build",
    "vercel-build": "cd vue-project && cp .env.production .env && npm install && npm run build && cp swagger-spec.json dist/ && cp swagger-ui.html dist/"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "dotenv": "^17.2.1",
    "node-fetch": "^3.3.2"
  },
  "keywords": [
    "api",
    "server",
    "supabase"
  ],
  "author": "Shinil Project",
  "license": "MIT"
}
