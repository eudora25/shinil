{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "vue-project/dist",
  "installCommand": "npm install && cd vue-project && npm install",
  "functions": {
    "api/docs.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/docs",
      "dest": "/api/docs"
    },
    {
      "src": "/api-docs", 
      "dest": "/api/docs"
    },
    {
      "src": "/swagger",
      "dest": "/api/docs"
    },
    {
      "src": "/swagger-ui.html",
      "dest": "/api/docs"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}