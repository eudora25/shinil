{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "vue-project/dist",
  "installCommand": "npm install && cd vue-project && npm install",

  "routes": [
    {
      "src": "/swagger-ui.html",
      "dest": "/swagger-ui.html"
    },
    {
      "src": "/swagger-spec.json",
      "dest": "/swagger-spec.json"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}