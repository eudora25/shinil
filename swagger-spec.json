    "/companies": {
      "get": {
        "tags": ["기본 정보"],
        "summary": "업체정보 조회",
        "description": "등록된 업체들의 정보를 페이지네이션과 함께 조회합니다.",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "required": false,
            "description": "페이지 번호 (기본값: 1)",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "default": 1
            },
            "example": 1
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "description": "페이지당 항목 수 (기본값: 100)",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "maximum": 1000,
              "default": 100
            },
            "example": 100
          }
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "업체정보 조회 성공",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "업체 목록 조회 성공"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string",
                            "description": "업체 ID"
                          },
                          "company_name": {
                            "type": "string",
                            "description": "업체명"
                          },
                          "business_registration_number": {
                            "type": "string",
                            "description": "사업자등록번호"
                          },
                          "representative_name": {
                            "type": "string",
                            "description": "대표자명"
                          },
                          "email": {
                            "type": "string",
                            "description": "이메일"
                          },
                          "mobile_phone": {
                            "type": "string",
                            "description": "휴대폰번호"
                          },
                          "status": {
                            "type": "string",
                            "description": "상태"
                          },
                          "created_at": {
                            "type": "string",
                            "format": "date-time",
                            "description": "생성일시"
                          }
                        }
                      }
                    },
                    "pagination": {
                      "type": "object",
                      "properties": {
                        "page": {
                          "type": "integer",
                          "description": "현재 페이지"
                        },
                        "limit": {
                          "type": "integer",
                          "description": "페이지당 항목 수"
                        },
                        "total": {
                          "type": "integer",
                          "description": "전체 항목 수"
                        },
                        "total_pages": {
                          "type": "integer",
                          "description": "전체 페이지 수"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "인증 실패",
            "content": {
              "application/json": {
                "schema": {
